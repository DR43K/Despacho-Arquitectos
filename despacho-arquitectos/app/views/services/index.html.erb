<p id="notice"><%= notice %></p>

<h1>Services</h1>

<table>
  <thead>
    <tr>
      <th>Idservicio</th>
      <th>Etiquetaservicio</th>
      <th>Estadoventa</th>
      <th>Estadocompra</th>
      <th>Descripcion</th>
      <th>Duracion</th>
      <th>Precioventa</th>
      <th>Precioventamin</th>
      <th>Categoria</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @services.each do |service| %>
      <tr>
        <td><%= service.idservicio %></td>
        <td><%= service.etiquetaservicio %></td>
        <td><%= service.estadoventa %></td>
        <td><%= service.estadocompra %></td>
        <td><%= service.descripcion %></td>
        <td><%= service.duracion %></td>
        <td><%= service.precioventa %></td>
        <td><%= service.precioventamin %></td>
        <td><%= service.categoria %></td>
        <td><%= link_to 'Show', service %></td>
        <td><%= link_to 'Edit', edit_service_path(service) %></td>
        <td><%= link_to 'Destroy', service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service', new_service_path %>
